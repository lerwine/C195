                                <ToggleGroup fx:id="activeToggleGroup" />
                            </toggleGroup>
                        </RadioButton>
                        <RadioButton fx:id="activeFalseRadioButton" mnemonicParsing="false" text="%no" toggleGroup="$activeToggleGroup" />
                    </children>
                    <GridPane.margin>
                        <Insets top="16.0" />
                    </GridPane.margin>
                </HBox>
                <Label styleClass="leftControlLabel" text="%address" GridPane.rowIndex="3">
