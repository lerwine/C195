                <RowConstraints valignment="CENTER" vgrow="SOMETIMES" />
                <RowConstraints valignment="TOP" vgrow="SOMETIMES" />
                <RowConstraints valignment="CENTER" vgrow="SOMETIMES" />
                <RowConstraints valignment="CENTER" vgrow="SOMETIMES" />
                <RowConstraints valignment="TOP" vgrow="SOMETIMES" />
                <RowConstraints valignment="TOP" vgrow="ALWAYS" />
            </rowConstraints>
            <children>
                <Label styleClass="leftControlLabel" text="%name" />
                <TextField fx:id="nameTextField" GridPane.columnIndex="1" />
                <Label fx:id="nameValidationLabel" styleClass="formControlValidationMessage" text="%nameCannotBeEmpty" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                <Label styleClass="leftControlLabel" text="%active" GridPane.rowIndex="2">
                    <GridPane.margin>
                        <Insets top="16.0" />
                    </GridPane.margin>
                </Label>
                <HBox spacing="16.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
                    <children>
                        <RadioButton fx:id="activeTrueRadioButton" mnemonicParsing="false" selected="true" text="%yes">
