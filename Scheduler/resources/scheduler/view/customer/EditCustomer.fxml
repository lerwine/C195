                <Label fx:id="cityValidationLabel" styleClass="formControlValidationMessage" text="City must be selected." GridPane.columnIndex="1" GridPane.rowIndex="7" />
                <Label styleClass="leftControlLabel" text="%phoneNumber" GridPane.rowIndex="10">
                    <GridPane.margin>
                        <Insets top="16.0" />
                    </GridPane.margin>
                </Label>
                <StackPane alignment="CENTER_LEFT" GridPane.columnIndex="1" GridPane.rowIndex="10" GridPane.valignment="TOP">
                    <children>
                        <Label fx:id="phoneNumberValueLabel" text="Label" />
                        <TextField fx:id="phoneNumberTextField" />
                    </children>
                    <GridPane.margin>
                        <Insets top="16.0" />
                    </GridPane.margin>
                </StackPane>
            </children>
            <padding>
                <Insets right="8.0" />
            </padding>
        </GridPane>
