                        <Insets top="16.0" />
                    </GridPane.margin>
                </Label>
                <StackPane alignment="CENTER_LEFT" GridPane.columnIndex="1" GridPane.rowIndex="11">
                    <children>
                        <Label fx:id="countryValueLabel" text="Label" wrapText="true" />
                        <ComboBox fx:id="countryComboBox" maxWidth="1.7976931348623157E308" />
                    </children>
                    <GridPane.margin>
                        <Insets top="16.0" />
                    </GridPane.margin>
                </StackPane>
                <ButtonBar maxHeight="-Infinity" GridPane.columnSpan="2" GridPane.rowIndex="13" GridPane.valignment="TOP">
                    <buttons>
                        <Button mnemonicParsing="false" onAction="#selectExistingButtonAction" text="Select Existing" />
                        <Button mnemonicParsing="false" onAction="#createNewButtonAction" text="Create New" />
                    </buttons>
                    <GridPane.margin>
                        <Insets top="16.0" />
