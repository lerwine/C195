<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>


<fx:root styleClass="mainFxmlClass" type="GridPane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <stylesheets>
      <URL value="@/scheduler/defaultStyles.css" />
   </stylesheets>
   <rowConstraints>
      <RowConstraints valignment="TOP" />
      <RowConstraints valignment="TOP" />
      <RowConstraints valignment="TOP" />
      <RowConstraints valignment="TOP" />
      <RowConstraints valignment="TOP" />
      <RowConstraints maxHeight="1.7976931348623157E308" minHeight="10.0" valignment="TOP" vgrow="ALWAYS" />
   </rowConstraints>
   <columnConstraints>
      <ColumnConstraints halignment="RIGHT" minWidth="10.0" />
      <ColumnConstraints hgrow="ALWAYS" maxWidth="1.7976931348623157E308" minWidth="10.0" />
   </columnConstraints>
   <children>
      <Label fx:id="logMessageLabel" styleClass="formControlLabel" text="%logMessage" />
      <TextField fx:id="logMessageTextField" editable="false" styleClass="formLabeledControl" GridPane.columnIndex="1" />
      <Label styleClass="formControlLabel" text="%type" GridPane.rowIndex="1" />
      <TextField fx:id="typeTextField" editable="false" styleClass="formLabeledControl" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Label fx:id="messageLabel" styleClass="formControlLabel" text="%message" GridPane.rowIndex="2" />
      <TextField fx:id="messgeTextField" editable="false" styleClass="formLabeledControl" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <Label fx:id="errorCodeLabel" styleClass="formControlLabel" text="%errorCode" GridPane.rowIndex="3" />
      <TextField fx:id="errorCodeTextField" editable="false" styleClass="formLabeledControl" GridPane.columnIndex="1" GridPane.rowIndex="3" />
      <Label fx:id="stateLabel" styleClass="formControlLabel" text="%state" GridPane.rowIndex="4" />
      <TextField fx:id="stateTextField" editable="false" styleClass="formLabeledControl" GridPane.columnIndex="1" GridPane.rowIndex="4" />
      <Accordion fx:id="detailAccordion" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" GridPane.columnSpan="2" GridPane.halignment="LEFT" GridPane.hgrow="ALWAYS" GridPane.rowIndex="5">
         <panes>
            <TitledPane fx:id="relatedExceptionsPane" text="%relatedExceptions" />
            <TitledPane fx:id="causedByPane" text="%causedBy" />
            <TitledPane fx:id="stackTracePane" animated="false" text="%stackTrace">
               <content>
                  <TextArea fx:id="stackTraceTextArea" editable="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" />
               </content>
            </TitledPane>
         </panes>
      </Accordion>
   </children>
</fx:root>
