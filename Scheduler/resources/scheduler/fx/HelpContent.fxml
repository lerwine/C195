<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>

<fx:root maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" styleClass="view-modal-mask" stylesheets="@../defaultStyles.css" type="BorderPane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <center>
        <BorderPane maxHeight="-Infinity" maxWidth="-Infinity" styleClass="bordered-section" BorderPane.alignment="CENTER">
            <top>
                <Label fx:id="titleLabel" text="%schedulerHelp">
                    <styleClass>
                        <String fx:value="header" />
                        <String fx:value="h1" />
                    </styleClass>
                </Label>
            </top>
            <center>
                <ScrollPane fx:id="contentScrollPane" pannable="true" styleClass="content" BorderPane.alignment="CENTER" />
            </center>
            <bottom>
                <ButtonBar styleClass="footer">
                    <buttons>
                        <Button mnemonicParsing="false" onAction="#onCloseButtonAction" text="%close" />
                    </buttons>
                </ButtonBar>
            </bottom>
            <styleClass>
                <String fx:value="mainFxmlClass" />
                <String fx:value="bordered" />
            </styleClass>
        </BorderPane>
    </center>
</fx:root>
