<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="600.0" prefWidth="800.0" styleClass="outerContainer" stylesheets="@defaultStyles.css"
      fx:controller="controller.EditCustomerController"
      xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label styleClass="header-section" text="Add New Customer" />
      <GridPane styleClass="content-section">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" />
          <ColumnConstraints hgrow="ALWAYS" />
            <ColumnConstraints hgrow="SOMETIMES" />
            <ColumnConstraints hgrow="ALWAYS" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints vgrow="ALWAYS" />
        </rowConstraints>
         <children>
            <Label styleClass="hLeftFieldLabel" text="Label" GridPane.valignment="BOTTOM" />
            <TextField styleClass="hFieldControl" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.valignment="BOTTOM" GridPane.vgrow="SOMETIMES" />
            <Label styleClass="hInnerFieldLabel" text="Label" GridPane.columnIndex="2" GridPane.valignment="BOTTOM" />
            <TextField styleClass="hFieldControl" GridPane.columnIndex="3" GridPane.hgrow="ALWAYS" GridPane.valignment="TOP" GridPane.vgrow="SOMETIMES" />
            <Label styleClass="error" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1" GridPane.valignment="TOP" GridPane.vgrow="ALWAYS" />
            <Label alignment="TOP_LEFT" styleClass="error" text="Label" GridPane.columnIndex="3" GridPane.halignment="LEFT" GridPane.rowIndex="1" GridPane.valignment="TOP" GridPane.vgrow="ALWAYS" />
         </children>
      </GridPane>
      <ButtonBar prefHeight="40.0" prefWidth="200.0" styleClass="command-section">
        <buttons>
          <Button mnemonicParsing="false" text="Save" />
            <Button mnemonicParsing="false" text="Cancel" />
        </buttons>
      </ButtonBar>
   </children>
</VBox>
