<VBox prefHeight="600.0" prefWidth="800.0" spacing="8.0" styleClass="outerContainer" stylesheets="@../../scheduler/defaultStyles.css" xmlns="http://javafx.com/javafx/8.0.171"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.appointment.EditAppointment">
    <children>
        <GridPane VBox.vgrow="ALWAYS">
            <columnConstraints>
                <ColumnConstraints hgrow="ALWAYS" />
                <ColumnConstraints />
                <ColumnConstraints hgrow="ALWAYS" />
                <ColumnConstraints />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints />
                <RowConstraints />
                <RowConstraints />
                <RowConstraints />
                <RowConstraints />
                <RowConstraints /><!--5-->
                <RowConstraints />
                <RowConstraints />
                <RowConstraints />
                <RowConstraints />
                <RowConstraints /><!--10-->
                <RowConstraints />
                <RowConstraints />
                <RowConstraints />
                <RowConstraints />
                <RowConstraints /><!--15-->
                <RowConstraints />
                <RowConstraints />
                <RowConstraints minHeight="10.0" vgrow="ALWAYS" />
                <RowConstraints />
                <RowConstraints /><!--20-->
            </rowConstraints>
            <children>
                <Label styleClass="formControlLabel" text="%customer" GridPane.columnSpan="2" />
                <Label styleClass="formControlLabel" text="%user" GridPane.columnIndex="2" GridPane.columnSpan="2">
                    <GridPane.margin>
                        <Insets left="8.0" />
                    </GridPane.margin>
                </Label>
                <ComboBox fx:id="customerComboBox" maxWidth="1.7976931348623157E308" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1">
                    <cellFactory>
                        <CustomerListCellFactory />
                    </cellFactory>
                    <buttonCell>
                        <CustomerListCell />
                    </buttonCell>
                </ComboBox>
                <Button maxWidth="-Infinity" mnemonicParsing="false" onAction="#addCustomerClick" prefWidth="38.0" text="%add" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                <ComboBox fx:id="userComboBox" maxWidth="1.7976931348623157E308" GridPane.columnIndex="2" GridPane.hgrow="SOMETIMES" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets left="8.0" />
                    </GridPane.margin>
                    <cellFactory>
                        <UserListCellFactory />
                    </cellFactory>
                    <buttonCell>
                        <UserListCell />
                    </buttonCell>
                </ComboBox>
                <Button maxWidth="-Infinity" mnemonicParsing="false" onAction="#addUserClick" prefWidth="38.0" text="%add" GridPane.columnIndex="3" GridPane.rowIndex="1" />
                <Label fx:id="customerValidationLabel" styleClass="formControlValidationMessage" text="%required" GridPane.rowIndex="2" />
                <Label fx:id="userValidationLabel" styleClass="formControlValidationMessage" text="%required" GridPane.columnIndex="2" GridPane.rowIndex="2">
                    <GridPane.margin>
                        <Insets left="8.0" />
                    </GridPane.margin>
                </Label>
                <Label styleClass="formControlLabel" text="%title" GridPane.columnSpan="4" GridPane.rowIndex="3">
                    <GridPane.margin>
                        <Insets top="8.0" />
                    </GridPane.margin>
                </Label>
                <TextField fx:id="titleTextField" GridPane.columnSpan="4" GridPane.rowIndex="4" />
                <Label fx:id="titleValidationLabel" styleClass="formControlValidationMessage" text="%required" GridPane.columnSpan="4" GridPane.rowIndex="5" />
