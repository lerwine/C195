                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" />
                        <ColumnConstraints hgrow="SOMETIMES" />
                        <ColumnConstraints hgrow="ALWAYS" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints />
                        <RowConstraints vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <Label styleClass="formControlLabel" text="%start" />
                        <VBox GridPane.rowIndex="1">
                            <children>
                                <DatePicker fx:id="startDatePicker">
                                    <VBox.margin>
                                        <Insets bottom="8.0" />
                                    </VBox.margin>
                                </DatePicker>
                                <HBox alignment="CENTER">
                                    <children>
                                        <ComboBox fx:id="startHourComboBox" minWidth="64.0" prefWidth="64.0">
                                            <cellFactory>
                                                <ZeroPadDigitListCellFactory />
                                            </cellFactory>
                                            <buttonCell>
                                                <ZeroPadDigitListCell />
                                            </buttonCell>
                                        </ComboBox>
                                        <Label text=":" />
                                        <ComboBox fx:id="startMinuteComboBox" minWidth="64.0" prefWidth="64.0">
                                            <cellFactory>
                                                <ZeroPadDigitListCellFactory />
                                            </cellFactory>
                                            <buttonCell>
                                                <ZeroPadDigitListCell />
                                            </buttonCell>
                                        </ComboBox>
                                    </children>
                                </HBox>
                                <Label fx:id="startValidationLabel" styleClass="formControlValidationMessage" text="%required" />
                            </children>
                        </VBox>
                        <Label styleClass="formControlLabel" text="%end" GridPane.columnIndex="1">
                            <GridPane.margin>
                                <Insets left="8.0" />
                            </GridPane.margin>
                        </Label>
                        <VBox GridPane.columnIndex="1" GridPane.rowIndex="1">
                            <children>
                                <DatePicker fx:id="endDatePicker">
                                    <VBox.margin>
                                        <Insets bottom="8.0" />
                                    </VBox.margin>
                                </DatePicker>
                                <HBox alignment="CENTER">
                                    <children>
                                        <ComboBox fx:id="endHourComboBox" minWidth="64.0" prefWidth="64.0">
                                            <cellFactory>
                                                <ZeroPadDigitListCellFactory />
                                            </cellFactory>
                                            <buttonCell>
                                                <ZeroPadDigitListCell />
                                            </buttonCell>
                                        </ComboBox>
                                        <Label text=":" />
                                        <ComboBox fx:id="endMinuteComboBox" minWidth="64.0" prefWidth="64.0">
                                            <cellFactory>
                                                <ZeroPadDigitListCellFactory />
                                            </cellFactory>
                                            <buttonCell>
                                                <ZeroPadDigitListCell />
                                            </buttonCell>
                                        </ComboBox>
                                    </children>
                                </HBox>
                                <Label fx:id="endValidationLabel" styleClass="formControlValidationMessage" text="%required" />
                            </children>
                            <padding>
                                <Insets left="8.0" right="8.0" />
                            </padding>
                        </VBox>
                        <Label styleClass="formControlLabel" text="%timeZone" GridPane.columnIndex="2" />
                        <VBox GridPane.columnIndex="2" GridPane.rowIndex="1" HBox.hgrow="SOMETIMES">
                            <children>
                                <ComboBox fx:id="timeZoneComboBox" maxWidth="1.7976931348623157E308">
                                    <cellFactory>
                                        <TimeZoneListCellFactory />
                                    </cellFactory>
                                    <buttonCell>
                                        <TimeZoneListCell />
                                    </buttonCell>
                                </ComboBox>
                                <HBox>
                                    <children>
                                        <Label fx:id="currentTimeZoneLabel" style="-fx-label-padding: 0 4 0 0;" text="%currentTimeZone">
                                            <styleClass>
                                                <String fx:value="formControlLabel" />
                                                <String fx:value="collapsed" />
                                            </styleClass>
                                        </Label>
                                        <Label fx:id="currentTimeZoneValue" styleClass="collapsed" text="currentTimeZone" wrapText="true" />
                                    </children>
                                </HBox>
                            </children>
                        </VBox>
                    </children>
                    <GridPane.margin>
                        <Insets top="8.0" />
                    </GridPane.margin>
                </GridPane>
                <HBox fx:id="showConflictsHBox" alignment="CENTER_LEFT" styleClass="collapsed" GridPane.columnSpan="4" GridPane.rowIndex="7">
                    <children>
                        <Label fx:id="conflictValidationLabel" styleClass="formControlValidationMessage" text="%conflictCustomer1User1" />
                        <Button mnemonicParsing="false" onAction="#showConflictsButtonClick" text="%show">
                            <HBox.margin>
                                <Insets left="8.0" />
                            </HBox.margin>
                        </Button>
                    </children>
                    <GridPane.margin>
                        <Insets top="8.0" />
                    </GridPane.margin>
                </HBox>
                <Label styleClass="formControlLabel" text="%type" GridPane.columnSpan="2" GridPane.rowIndex="8">
                    <GridPane.margin>
                        <Insets top="8.0" />
                    </GridPane.margin>
                </Label>
                <Label fx:id="locationLabel" styleClass="formControlLabel" text="%location" GridPane.columnIndex="2" GridPane.columnSpan="2" GridPane.rowIndex="8">
                    <GridPane.margin>
                        <Insets left="8.0" top="8.0" />
                    </GridPane.margin>
                </Label>
                <ComboBox fx:id="typeComboBox" maxWidth="1.7976931348623157E308" GridPane.columnSpan="2" GridPane.hgrow="SOMETIMES" GridPane.rowIndex="9" GridPane.valignment="TOP">
                    <cellFactory>
                        <AppointmentTypeListCellFactory />
                    </cellFactory>
                    <buttonCell>
                        <AppointmentTypeListCell />
                    </buttonCell>
                </ComboBox>
                <TextArea fx:id="locationTextArea" prefRowCount="4" GridPane.columnIndex="2" GridPane.columnSpan="2" GridPane.rowIndex="9">
                    <GridPane.margin>
                        <Insets left="8.0" />
                    </GridPane.margin>
                </TextArea>
                <TextField fx:id="phoneTextField" styleClass="collapsed" GridPane.columnIndex="2" GridPane.columnSpan="2" GridPane.rowIndex="9" GridPane.valignment="TOP">
                    <GridPane.margin>
                        <Insets left="8.0" />
                    </GridPane.margin>
                </TextField>
                <Label fx:id="implicitLocationLabel" styleClass="collapsed" GridPane.columnIndex="2" GridPane.columnSpan="2" GridPane.rowIndex="9" GridPane.valignment="TOP" />
                <Label fx:id="locationValidationLabel" styleClass="formControlValidationMessage" text="%required" GridPane.columnIndex="2" GridPane.columnSpan="2" GridPane.rowIndex="10">
                    <GridPane.margin>
                        <Insets left="8.0" />
                    </GridPane.margin>
                </Label>
                <Label fx:id="urlLabel" text="%meetingUrl" GridPane.columnSpan="4" GridPane.rowIndex="11">
                    <GridPane.margin>
                        <Insets top="8.0" />
                    </GridPane.margin>
                    <styleClass>
                        <String fx:value="formControlLabel" />
                        <String fx:value="collapsed" />
                    </styleClass>
                </Label>
                <TextField fx:id="urlTextField" styleClass="collapsed" GridPane.columnSpan="4" GridPane.rowIndex="12" />
                <Label fx:id="urlValidationLabel" text="%invalidUrl" GridPane.columnSpan="4" GridPane.rowIndex="13">
                    <styleClass>
                        <String fx:value="formControlValidationMessage" />
                        <String fx:value="collapsed" />
                    </styleClass>
                </Label>
                <Label styleClass="formControlLabel" text="%pointOfContact" GridPane.columnSpan="4" GridPane.rowIndex="14">
                    <GridPane.margin>
                        <Insets top="8.0" />
                    </GridPane.margin>
                </Label>
                <TextField fx:id="contactTextField" GridPane.columnSpan="4" GridPane.rowIndex="15" />
                <Label fx:id="contactValidationLabel" styleClass="formControlValidationMessage" text="%required" GridPane.columnSpan="4" GridPane.rowIndex="16" />
                <Label styleClass="formControlLabel" text="%description" GridPane.columnSpan="4" GridPane.rowIndex="17">
                    <GridPane.margin>
                        <Insets top="8.0" />
                    </GridPane.margin>
                </Label>
                <TextArea fx:id="descriptionTextArea" prefRowCount="4" GridPane.columnSpan="4" GridPane.rowIndex="18" />
                <Label fx:id="createdLabel" styleClass="formControlLabel" text="%created" GridPane.rowIndex="19">
                    <GridPane.margin>
                        <Insets top="8.0" />
                    </GridPane.margin>
                </Label>
                <Label fx:id="lastUpdateLabel" styleClass="formControlLabel" text="%updated" GridPane.columnIndex="2" GridPane.rowIndex="19">
                    <GridPane.margin>
                        <Insets left="8.0" top="8.0" />
                    </GridPane.margin>
                </Label>
                <HBox GridPane.rowIndex="20">
                    <children>
                        <Label fx:id="createDateValue" text="12/10/2019 21:34" />
                        <Label fx:id="createdByLabel" styleClass="formControlLabel" text="%by">
                            <opaqueInsets>
                                <Insets left="8.0" />
                            </opaqueInsets>
                            <HBox.margin>
                                <Insets left="8.0" />
                            </HBox.margin>
                        </Label>
                        <Label fx:id="createdByValue" text="Label" />
                    </children>
                </HBox>
                <HBox GridPane.columnIndex="2" GridPane.rowIndex="20">
                    <children>
                        <Label fx:id="lastUpdateValue" text="12/10/2019 21:34" />
                        <Label fx:id="lastUpdateByLabel" styleClass="formControlLabel" text="%by">
                            <opaqueInsets>
                                <Insets left="8.0" />
                            </opaqueInsets>
                            <HBox.margin>
                                <Insets left="8.0" />
                            </HBox.margin>
                        </Label>
                        <Label fx:id="lastUpdateByValue" text="Label" />
                    </children>
                    <GridPane.margin>
                        <Insets left="8.0" />
                    </GridPane.margin>
                </HBox>
            </children>
        </GridPane>
        <ButtonBar>
            <buttons>
                <Button fx:id="saveChangesButton" mnemonicParsing="false" onAction="#saveChangesClick" text="%save" />
                <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancelClick" text="%cancel" />
            </buttons>
        </ButtonBar>
    </children>
    <padding>
        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
    </padding>
</VBox>
