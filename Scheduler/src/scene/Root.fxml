<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox id="AnchorPane" prefHeight="600.0" prefWidth="800.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" fx:controller="scene.RootController">
    <stylesheets>
        <URL value="@/scheduler/defaultStyles.css"/>
    </stylesheets>
    <children>
        <MenuBar>
            <menus>
                <Menu fx:id="appointmentsMenu" mnemonicParsing="false" text="%appointments">
                    <items>
                        <MenuItem fx:id="newAppointmentMenuItem" mnemonicParsing="false" onAction="#newAppointmentMenuItemClick" text="%new" />
                        <MenuItem fx:id="allAppointmentsMenuItem" mnemonicParsing="false" onAction="#allAppointmentsMenuItemClick" text="%allAppointments" />
                    </items>
                </Menu>
                <Menu fx:id="customersMenu" mnemonicParsing="false" text="%customers">
                    <items>
                        <MenuItem fx:id="newCustomerMenuItem" mnemonicParsing="false" onAction="#newCustomerMenuItemClick" text="%new" />
                        <MenuItem fx:id="allCustomersMenuItem" mnemonicParsing="false" onAction="#allCustomersMenuItemClick" text="%allCustomers" />
                    </items>
                </Menu>
                <Menu fx:id="addressMenu" mnemonicParsing="false" text="%address">
                    <items>
                        <MenuItem fx:id="newCountryMenuItem" mnemonicParsing="false" onAction="#newCountryMenuItemClick" text="%newCountry" />
                        <MenuItem fx:id="newCityMenuItem" mnemonicParsing="false" onAction="#newCityMenuItemClick" text="%newCity" />
                        <MenuItem fx:id="newAddressMenuItem" mnemonicParsing="false" onAction="#newAddressMenuItemClick" text="%newAddress" />
                        <MenuItem fx:id="allCountriesMenuItem" mnemonicParsing="false" onAction="#allCountriesMenuItemClick" text="%allCountries" />
                    </items>
                </Menu>
                <Menu fx:id="usersMenu" mnemonicParsing="false" text="%users">
                    <items>
                        <MenuItem fx:id="newUserMenuItem" mnemonicParsing="false" onAction="#newUserMenuItemClick" text="%new" />
                        <MenuItem fx:id="allUsersMenuItem" mnemonicParsing="false" onAction="#allUsersMenuItemClick" text="%allUsers" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <BorderPane fx:id="contentPane" />
    </children>
</VBox>
